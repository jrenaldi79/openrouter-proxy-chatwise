{
  "name": "openrouter-proxy",
  "version": "1.0.0",
  "description": "TypeScript/Node.js proxy server for OpenRouter API with credit transformation and transparent passthrough",
  "main": "dist/src/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "start": "node dist/src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --config=jest.unit.config.js",
    "test:unit:watch": "jest --config=jest.unit.config.js --watch",
    "test:contract": "jest tests/contract --testTimeout=10000",
    "test:integration": "jest tests/integration --testTimeout=15000",
    "test:performance": "jest --config=jest.performance.config.js",
    "test:production": "node scripts/test-production.js",
    "test:production:jest": "jest --config=jest.production.config.js",
    "test:real-api": "jest --config=jest.production.config.js",
    "test:all": "jest",
    "test:local": "npm run test:unit && npm run test:contract && npm run test:integration",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openrouter",
    "proxy",
    "api",
    "typescript",
    "nodejs",
    "express"
  ],
  "author": "John Renaldi",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@langfuse/otel": "^4.2.0",
    "@langfuse/tracing": "^4.2.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uuid": "^8.3.2",
    "weave": "^0.9.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "nock": "^14.0.10",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "typescript-eslint": "^8.44.1"
  }
}
